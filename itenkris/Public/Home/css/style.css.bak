#news{
    width:100%;
	height:560px;	
	background-color:#C2C6CC;
}
#new-1,#new-2,#new-3,#new-4{
    width:200px;
	height:400px;	
	float:left;	
	margin-top:50px;
}
#new-1{
    margin-left:80px;
}
#new-2,#new-3,#new-4{
    margin-left:30px;
}
#news-pic{
    height:150px;
}
#news-pic img{
    width:200px;
	margin-left:0px;
	height:150px; 
}
#news-title{
    height:80px;
	text-align:left;
	font-size:16px;
	font:bold;
	font-family:Calibri;
	margin-top:5px;
}
/*未被点击时超链接文字无下划线,显示为黑色*/
#news-title a:link {text-decoration:none;color:#040404;}
/*鼠标放在链接上时有下划线,链接文字显示为红色*/
#news-title a:hover {text-decoration:underline;color:#F31010;}
/*当点击链接时,即链接被激活,链接无下划线,显示为黄色*/
#news-title a:active{text-decoration:none;color:#ECF310;}
/*当点击链接后,也就是链接已被访问过后,链接无下划线,显示为绿色*/
#news-title a:visited {text-decoration:none;color:#050505;}
#news-content{
    text-align:left;
	font-size:15px;
	height:150px;
	font-family:Calibri;
	margin-top:5px;
	overflow:hidden;
}
