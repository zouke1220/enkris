/****************头部****************/
*{  margin:0 auto;}
html,body{
    margin: 0 auto;
    padding: 0;
	text-align:center;
	width:1200px;
	height:100%;
    font-size:16px;
	/*用于使页面内进行position定位的元素在页面缩放过程,保持相对位置不变*/
	position:relative;
	background:#E4DFDF;
 }
/*主面板样式*/
#Contact {
    width:1200px;
	height:auto;
    margin:0px auto;/*主面板DIV居中*/
}
/*清理浮动*/
.clear{ 
    clear:both;
} 
/*顶部面板样式*/
#logo,#menu li,#switch{
    float:left;
}
#header {
    width:100%;
    height:60px;
	/*设置div的背景图片*/
    background-color:#222222;
}
#logo{
    width:40px;
    height:50px;
	margin-top:10px;
	margin-left:70px;
}
#menu{
    float:left;
    height:60px;
	width:auto;
}
/*列表项样式*/
#menu ul li{
    float:left;
    width:80px;
    height:60px;
    text-align:center;
    list-style:none; /*去掉列表项的样式(小圆点)*/
    line-height: 60px;/*行高*/
    font-weight: bold;/*字体加粗*/   
    font-size: 20px;
	line-height:60px;
	padding-left:30px;
}
#menu ul{
     font-family:Agency FB;
}
#menu ul li a{
    text-decoration:none;/*去掉a标签的下划线*/		
	font-family:Agency FB;
	color:#FFFFFF;
}
